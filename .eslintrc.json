{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"        
    ],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", { "code": 105 }],
        "require-jsdoc" : 0,
        "new-cap": 0
    }
}
